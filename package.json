{
  "name": "project_start",
  "version": "1.0.0",
  "description": "Boilerplate to quickly get started with a project",
  "main": "index.html",
  "scripts": {
    "start": "npm-run-all -p liveserver watch",
    "liveserver": "browser-sync start --server --files '**/*' ",
    "watch": "node-sass sass/style.scss css/style.css -w"
  },
  "author": "Kriss Steindals <ksteindals@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "browser-sync": "^2.26.7",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5"
  }
}
